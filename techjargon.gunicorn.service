[Unit]
Description=techjargon daemon
After=network.target

[Service]
User=techjargon
Group=www-data
WorkingDirectory=/var/www/techjargon
ExecStart=gunicorn --access-logfile - --workers 3 --bind unix:/var/www/techjargon/techjargon.sock /var/www/techjargon/techjargon-app/techjargon.wsgi:application

[Install]
WantedBy=multi-user.target
